<!DOCTYPE HTML>
<HTML>
	<HEAD>
		<META CHARSET='UTF-8'/>
		<meta http-equiv="Content-Language" content="zh-CN" />
		<TITLE>yData</TITLE>
		<script src="js/jquery-1.11.0-beta3.js"></script>
	</HEAD>
	<BODY>
		<H1> I WANT TO GET DATAS! </H1>
		<FORM>
			<INPUT id='txt_webSite' type='text' value ='www.china-designer.com' style="width:500px;" autofocus required/>
			<INPUT type='button' value='submit' onClick='getData();' style="width:100px;" />
		</FORM>
		<div id="newHtml">
		</div>
	<!--post提交数据的方式-->
	<script>

	function getData(){
		$.ajax({
			url: ""+$("#txt_webSite").val(),
			cache: false,
			success: function(html){
			  $("#newHtml").append=html;
		    }
		});
	};
	</script>
	</BODY>
</HTML>